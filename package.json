{
  "name": "@rysh/chaff-files",
  "version": "1.1.4",
  "description": "Generates fake hacker responses based on request paths.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "ts-node-dev --respawn src/server/dev-server.ts",
    "build": "rm -rf dist && tsc --emitDeclarationOnly && esbuild src/index.ts --bundle --minify --sourcemap --platform=node --format=cjs --outfile=dist/index.js",
    "test": "jest",
    "prepare": "husky install"
  },
  "keywords": [
    "chaff",
    "honeypot",
    "security"
  ],
  "author": "rysh",
  "license": "ISC",
  "type": "commonjs",
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.5.2",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "esbuild": "^0.25.10",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "express": "^5.1.0",
    "husky": "^9.1.7",
    "jest": "^30.1.3",
    "lint-staged": "^16.2.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.1"
  }
}
